{
  "name": "express-model-binding",
  "version": "1.1.0",
  "description": "Laravel-style route model binding for Express.js with support for multiple ORMs (Knex, Mongoose, TypeORM, Sequelize, Prisma)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./knex": {
      "types": "./dist/adapters/KnexAdapter.d.ts",
      "require": "./dist/adapters/KnexAdapter.js",
      "import": "./dist/adapters/KnexAdapter.mjs"
    },
    "./mongoose": {
      "types": "./dist/adapters/MongooseAdapter.d.ts",
      "require": "./dist/adapters/MongooseAdapter.js",
      "import": "./dist/adapters/MongooseAdapter.mjs"
    },
    "./typeorm": {
      "types": "./dist/adapters/TypeORMAdapter.d.ts",
      "require": "./dist/adapters/TypeORMAdapter.js",
      "import": "./dist/adapters/TypeORMAdapter.mjs"
    },
    "./sequelize": {
      "types": "./dist/adapters/SequelizeAdapter.d.ts",
      "require": "./dist/adapters/SequelizeAdapter.js",
      "import": "./dist/adapters/SequelizeAdapter.mjs"
    },
    "./prisma": {
      "types": "./dist/adapters/PrismaAdapter.d.ts",
      "require": "./dist/adapters/PrismaAdapter.js",
      "import": "./dist/adapters/PrismaAdapter.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:dev": "tsup",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "typedoc --out docs src/index.ts",
    "prisma:generate": "prisma generate --schema=./tests/fixtures/prisma/schema.prisma",
    "size": "npm pack --dry-run"
  },
  "keywords": [
    "express",
    "expressjs",
    "model",
    "binding",
    "route",
    "orm",
    "prisma",
    "typeorm",
    "sequelize",
    "mongoose",
    "knex",
    "laravel",
    "middleware",
    "typescript",
    "eloquent",
    "route-model-binding",
    "model-binding",
    "auto-resolve",
    "parameter-binding"
  ],
  "author": {
    "name": "Sepehr Mohseni",
    "email": "isepehrmohseni@gmail.com",
    "url": "https://github.com/sepehr-mohseni"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sepehr-mohseni/express-model-binding.git"
  },
  "bugs": {
    "url": "https://github.com/sepehr-mohseni/express-model-binding/issues"
  },
  "homepage": "https://github.com/sepehr-mohseni/express-model-binding#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "knex": {
      "optional": true
    },
    "mongoose": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    },
    "sequelize": {
      "optional": true
    },
    "@prisma/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prisma/client": "^5.7.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "better-sqlite3": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.0.3",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.6",
    "typeorm": "^0.3.19",
    "typescript": "^5.3.3"
  }
}
